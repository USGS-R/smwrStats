citHeader("Suggested citation for USGSwsStats:")

citEntry(entry = "TechReport",
  title        = "USGSstats--An R package for the analysis of hydrologic data, Version 0.6",
  author       = personList(as.person("Dave Lorenz")),
  institution  = "U.S. Geological Survey",
  series       = "Open File report",
  year         = "in preparation",

  textVersion  =
  paste("Lorenz, D.L., in preparation",
        "USGSwsStats--An R package for the analysis of hydrologic data, Version 0.6:",
        sep="")
)
